<h1>Agregar Productos</h1>
    <button>
        <a href="http://localhost:8080/api/products">inicio</a>
    </button>
<div>
    <h2>Crear producto</h2>
    <form action="">
        <label for="name">
            Nombre del producto
        </label>
        <input type="text" name="name" id="nameProduct"  />
        <label for="id">
            Id del producto
        </label>
        <input type="number" name="id" id="idProduct"  />

        <label for="descript">Descripcion del producto
        </label>
        <input type="text" name="descript" id="descriptProduct" />

        <label for="code">Codigo
        </label>
        <input type="text" name="code" id="codeProduct" />

        <label for="price">Precio
        </label>
        <input type="text" name="price" id="priceProduct" />

        <label for="stock">Cantidad de stock
        </label>
        <input type="text" name="stock" id="stockProduct" />

        <label for="category">Categoria
        </label>
        <input type="text" name="category" id="categoryProduct" />

        <button type="submit" id="newProduct">Crear nuevo producto</button>
        
    </form>
</div>

